{
    "AWSEBDockerrunVersion":"2",
    "containerDefinitions": [
        {
            "name": "client",
            "image": "sekhar92/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "sekhar92/multi-server",
            "hostname": "api",
            "essential": false 
        },
        {
            "name": "worker",
            "image": "sekhar92/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "sekhar92/multi-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": "80",
                    "containerPosr": "80"
                }
            ],
            "links": ["client", "server"]
        }
    ]
}